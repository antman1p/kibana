{
  "$schema": "https://raw.githubusercontent.com/nrwl/nx/refs/heads/master/packages/nx/schemas/project-schema.json",
  "name": "@kbn/resolver-test-plugin",
  "namedInputs": {
    "src": [
      "{projectRoot}/**/*.ts",
      "{projectRoot}/**/*.tsx",
      "!{projectRoot}/target/**/*"
    ]
  },
  "targets": {
    "typecheck": {
      "inputs": [
        "src",
        "^src",
        "global:tsConfigs",
        "^global:tsConfigs"
      ],
      "outputs": [
        "{projectRoot}/target"
      ]
    },
    "lint": {}
  },
  "projectType": "application",
  "sourceRoot": "x-pack/test/plugin_functional/plugins/resolver_test"
}