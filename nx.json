{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "cli": {
    "packageManager": "yarn"
  },
  "parallel": 5,
  "useDaemonProcess": true,
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "cacheableOperations": ["build", "test", "lint"]
      }
    }
  },
  "plugins": [],
  "namedInputs": {
    "global:ignoreConfigs": [
      "!{workspaceRoot}/nx.json",
      "!{workspaceRoot}/package.json",
      "!{workspaceRoot}/yarn.lock"
    ],
    "global:tsConfigs": [
      "{projectRoot}/tsconfig.json",
      "{projectRoot}/tsconfig.type_check.json",
      "{workspaceRoot}/tsconfig.base.json",
      "{workspaceRoot}/tsconfig.base.type_check.json",
      "!{workspaceRoot}/nx.json",
      "!{workspaceRoot}/package.json",
      "!{workspaceRoot}/yarn.lock"
    ]
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "typecheck": {
      "executor": "@kbn/nx:typecheck-js",
      "dependsOn": ["^@kbn/nx", "^typecheck"],
      "inputs": ["src", "^src", "global:tsConfigs", "^global:tsConfigs"],
      "outputs": ["{projectRoot}/target"],
      "options": {},
      "cache": true
    },
    "jest": {
      "executor": "@nx/jest:jest",
      "inputs": ["src", "global:ignoreConfigs"],
      "options": {
        "passWithNoTests": true,
        "runInBand": true
      },
      "cache": true
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "inputs": ["src", "global:ignoreConfigs"],
      "options": {
        "cache": true,
        "ignorePath": ".eslintignore",
        "lintFilePatterns": [
          "{projectRoot}/**/*.js",
          "{projectRoot}/**/*.mjs",
          "{projectRoot}/**/*.cjs",
          "{projectRoot}/**/*.ts",
          "{projectRoot}/**/*.tsx"
        ]
      },
      "cache": true
    }
  },
  "_nxCloudId": "675840972b7c9c798b3d1e9f"
}
